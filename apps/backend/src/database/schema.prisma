generator client {
  provider = "prisma-client"
  output   = "./generated"
}

datasource db {
  provider = "postgresql"
}

model AuthUser {
  id String @id @default(uuid())

  serviceId String @unique
  memberId  String @unique

  username String @unique

  accessToken  String
  refreshToken String?
}
